{
  "name": "pygrid-admin",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "sh ./scripts/deploy-node-type.sh",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "pretty-quick --staged",
    "test:unit": "jest --watch",
    "coverage": "jest --coverage",
    "export": "next export",
    "build:tailwind": "postcss src/styles/globals.css -o src/styles/tailwind.output.css",
    "watch:tailwind": "postcss --verbose --watch src/styles/globals.css -o src/styles/tailwind.output.css",
    "cy:open": "cypress open",
    "cy:install": "cypress install",
    "cy:run": "cypress run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run format"
    }
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.5",
    "humps": "^2.0.1",
    "jwt-decode": "^3.1.2",
    "lodash.chunk": "^4.2.0",
    "lodash.take": "^4.1.1",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "17.0.2",
    "react-hook-form": "^6.15.3",
    "react-query": "3.8.2",
    "tailwindcss": "^2.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/classnames": "^2.2.11",
    "@types/humps": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "autoprefixer": "^10.2.4",
    "babel-jest": "^26.6.3",
    "browserslist": "^4.16.5",
    "cypress": "^8.3.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.3"
  }
}
