{
  "name": "pygrid-ui",
  "version": "0.7.0-beta.41",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "sh ./scripts/deploy-node-type.sh",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:unit": "jest --watch",
    "coverage": "jest --coverage",
    "export": "next export",
    "build:tailwind": "postcss src/styles/globals.css -o src/styles/tailwind.output.css",
    "watch:tailwind": "postcss --verbose --watch src/styles/globals.css -o src/styles/tailwind.output.css",
    "cypress:open": "export NEXT_PUBLIC_HOST=\"http://localhost:${HTTP_PORT-8081}\" && cypress open",
    "cypress:run": "export NEXT_PUBLIC_HOST=\"http://localhost:${HTTP_PORT-8081}\" && cypress run",
    "format": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run format"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "dayjs": "1.10.5",
    "jwt-decode": "3.1.2",
    "ky": "^0.28.5",
    "ky-universal": "^0.9.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "next": "^12.1.6",
    "nookies": "^2.5.2",
    "react": "^18.1.0",
    "react-animate-height": "^2.0.23",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.16.2",
    "react-query": "^3.24.3",
    "react-table": "^7.7.0",
    "react-tippy": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@fortawesome/fontawesome-common-types": "^0.3.0-beta2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.5",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.2",
    "@types/react-table": "^7.7.6",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^26.6.3",
    "browserslist": "^4.16.5",
    "cypress": "^8.7.0",
    "eslint": "^7.19.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "resolve-from": "^5.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.2.3"
  }
}
