{
  "name": "pygrid-ui",
  "version": "0.8.6-beta.1",
  "private": true,
  "scripts": {
    "dev": "pnpm i && vite dev --host --port 80",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 80",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "test:e2e": "playwright test",
    "test:unit": "vitest run",
    "format": "prettier --write --plugin-search-dir=. ./**/*.{js,ts,html,css,svelte}",
    "prepare": "svelte-kit sync"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.1",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "1.25.2",
    "@types/cookie": "^0.5.3",
    "@types/libsodium-wrappers": "0.7.11",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.13",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.2",
    "svelte-preprocess": "^4.10.7",
    "svelte-search": "^2.0.1",
    "tailwindcss": "3.3.1",
    "typescript": "^4.9.5",
    "typescript-svelte-plugin": "^0.3.34",
    "vite": "^4.5.2",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "dependencies": {
    "capnp-ts": "^0.7.0",
    "just-debounce-it": "^3.2.0",
    "ky": "^0.33.3",
    "libsodium": "0.7.11",
    "libsodium-wrappers": "0.7.11",
    "prismjs": "^1.29.0",
    "semver": "^7.5.4",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "overrides": {
      "undici@<5.26.2": ">=5.26.2"
    }
  }
}
