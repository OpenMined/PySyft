[flake8]
ignore = N801, N802, N805, N806, N812, N803, N804
         F405, F403, F811, F841,
         E203, E266, E722, E731, E741
         C901,
         W605, W503
         D100, D101, D102, D103, D104, D105, D107,
         D200, D202, D205, D209, D210, D400, D402,
         D403,D401, D412
max-line-length = 100
exclude = .git,.eggs,__pycache__,build,dist,venv
max-complexity = 11
show-source = true
statistics = true
count = true
